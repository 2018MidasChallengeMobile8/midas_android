<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorGray2</item>
        <item name="colorPrimaryDark">@color/colorBlack</item>
        <item name="colorAccent">@color/colorGray2</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="TabStyle">
        <item name="android:textSize" tools:ignore="SpUsage">13dp</item>
        <item name="android:textColor">@color/colorWhite</item>
    </style>

    <style name="FloatingActionButtonStyle">
        <item name="colorAccent">@color/colorRed2</item>
    </style>
</resources>
